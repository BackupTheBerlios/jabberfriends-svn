RewriteRule ^en/logout.htm$ logout.php?lang=en
RewriteRule ^en/options.htm$ options.php?lang=en
RewriteRule ^en/change_details.htm$ changedetails.php?lang=en
RewriteRule ^en/change_tags.htm$ changetags.php?lang=de
RewriteRule ^en/members/$ members.php?lang=en
RewriteRule ^en/members$ members.php?lang=en
RewriteRule ^en/members/search.htm$ search.php?lang=en&search=normal
RewriteRule ^en/members/extended_search.htm$ search.php?lang=en&search=extended
