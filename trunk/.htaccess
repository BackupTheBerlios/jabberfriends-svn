RewriteEngine on
RewriteBase /
RewriteRule ^de/mitglieder/([0-9]+)-([^/\:]+).htm$ user_details.php?lang=de&id=$1
RewriteRule ^de/publickeys/([0-9]+)-([^/\:]+).htm$ publickey.php?lang=de&id=$1
RewriteRule ^de/registrieren.htm$ register.php?lang=de
RewriteRule ^de/$ index2.php?lang=de
RewriteRule ^de$ index2.php?lang=de
RewriteRule ^de/anmelden.htm$ login.php?lang=de
RewriteRule ^de/abmelden.htm$ logout.php?lang=de
RewriteRule ^de/optionen.htm$ options.php?lang=de
RewriteRule ^de/angaben_aendern.htm$ changedetails.php?lang=de
RewriteRule ^de/tags_aendern.htm$ changetags.php?lang=de
